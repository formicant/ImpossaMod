    MODULE Colour

; ink colours on black paper

; non-bright
black       EQU #00
blue        EQU #01
red         EQU #02
magenta     EQU #03
green       EQU #04
cyan        EQU #05
yellow      EQU #06
white       EQU #07

; bright
brBlue      EQU #41
brRed       EQU #42
brMagenta   EQU #43
brGreen     EQU #44
brCyan      EQU #45
brYellow    EQU #46
brWhite     EQU #47

    ENDMODULE
